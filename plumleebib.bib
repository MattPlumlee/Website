@article{plumlee2019computer,
author = {Plumlee, Matthew},
title = {Computer model calibration with confidence and consistency},
journal = {Journal of the Royal Statistical Society: B},
volume = {81},
number = {3},
pages = {519-545},
keywords = {Frequentist, Inverse problem, Model discrepancy, Model inadequacy, Model misspecification, Non-linear regression, Uncertainty quantification},
doi = {https://doi.org/10.1111/rssb.12314},
url = {https://rss.onlinelibrary.wiley.com/doi/abs/10.1111/rssb.12314},
eprint = {https://rss.onlinelibrary.wiley.com/doi/pdf/10.1111/rssb.12314},
abstract = {Summary The paper proposes and examines a calibration method for inexact models. The method produces a confidence set on the parameters that includes the best parameter with a desired probability under any sample size. Additionally, this confidence set is shown to be consistent in that it excludes suboptimal parameters in large sample environments. The method works and the results hold with few assumptions; the ideas are maintained even with discrete input spaces or parameter spaces. Computation of the confidence sets and approximate confidence sets is discussed. The performance is illustrated in a simulation example as well as two real data examples.},
year = {2019},
  highlightmethod={1},
  pdf = {https://github.com/MattPlumlee/Website/raw/master/papers/plumlee2019computer.pdf}
}

